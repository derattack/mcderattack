          "_customHTML": "<section class=\"firmm4_form3 form3\" group=\"Forms\" plugins=\"formstyler, datepicker\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n<!--         <input type=\"range\" inline title=\"Width\" name=\"formWidth\" min=\"4\" max=\"10\" step=\"1\" value=\"6\"> -->\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked>\n        <input type=\"color\" title=\"Color BG\" value=\"#ffffff\" name=\"colorBg\">\n        <header>Form</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showFormTitle\" checked>\n        <input type=\"color\" title=\"From\" value=\"#ffffff\" name=\"formColor\">\n        <input type=\"color\" title=\"Border\" value=\"#e0e5ff\" name=\"borderColor\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/firmm4/components/_images/background/background.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"hidden\" name=\"usedRecaptcha\" value=\"false\">\n    </mbr-parameters>\n    \n    <div class=\"mbr-overlay\" mbr-if=\"(overlay && bg.type=='image')\"></div>\n    \n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"signup_wrap\">\n            <div class=\"row justify-content-center\">\n                <div class=\"col-lg-5 col-md-5 col-12\" mbr-if=\"showTitle || showSubtitle || showText || showButtons\">\n                    <div class=\"title_block\">\n                        <h3 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">\n                            <b>Already have an account?</b>\n                        </h3>\n                        <h4 class=\"mbr-section-subtitle mbr-fonts-style mbr-light\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">\n                            Log in now and enjoy our amazing products\n                        </h4>\n                        <div class=\"card_body\" mbr-if=\"showText\">\n                            <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list, .mbr-section-btn\" data-multiline mbr-article>\n                                <li class=\"mbr-text item-wrap\"><span class=\"mobi-mbri mobi-mbri-success mbr-iconfont\"></span>Lorem ipsum inventore</li>\n                                <li class=\"mbr-text item-wrap\"><span class=\"mobi-mbri mobi-mbri-success mbr-iconfont\"></span>Dolor sit amet</li>\n                                <li class=\"mbr-text item-wrap\"><span class=\"mobi-mbri mobi-mbri-success mbr-iconfont\"></span>Consectetur adipisicing elit</li>\n                            </ul>\n                        </div>\n                        <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a class=\"btn btn-sm btn-primary\" href=\"#\" data-app-placeholder=\"Type Text\">Sign In</a></div>\n                    </div>\n                </div>\n                \n                <div class=\"col-lg-7 col-md-7 col-12\">\n                    <div class=\"form_wrap\">\n                        <h3 class=\"form_title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showFormTitle\" data-app-selector=\".form_title\">\n                            <b>Sign Up</b>\n                        </h3>\n                        <div mbr-form class=\"mbr-form\">\n                            <!--Formbuilder Form-->\n                            <form action name=\"Form Name\" method=\"POST\" class=\"mbr-form form-with-styler mx-auto\">\n                                <div class>\n                                    <div hidden=\"hidden\" data-form-alert class=\"alert alert-success col-12\">Thanks for filling out the form!</div>\n                                    <div hidden=\"hidden\" data-form-alert-danger class=\"alert alert-danger col-12\">Oops...! some problem!</div>\n                                </div>\n                                <div class=\"dragArea\">\n                                    <div class=\"col-lg-12 col-md-12 col-sm-12 form-group\" data-for=\"name\">\n                                        <label mbr-text mbr-theme-style=\"display-4\" for=\"name\" class=\"form-control-label mbr-fonts-style\">Username</label>\n                                        <input type=\"text\" name=\"name\" placeholder=\"Name\" data-form-field=\"name\" mbr-theme-style=\"display-7\" class=\"form-control\" value>\n                                    </div>\n                                    <div class=\"col-lg-12 col-md-12 col-sm-12 form-group\" data-for=\"email\">\n                                        <label mbr-text mbr-theme-style=\"display-4\" for=\"email\" class=\"form-contr